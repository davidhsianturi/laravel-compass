{
    "name": "davidhsianturi/laravel-compass",
    "description": "An elegant REST assistent for the Laravel framework.",
    "keywords": [
        "laravel",
        "API",
        "testing",
        "documentation"
    ],
    "homepage": "https://github.com/davidhsianturi/laravel-compass",
    "license": "MIT",
    "authors": [
        {
            "name": "David H. Sianturi",
            "email": "davidhsianturi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "mpociot/documentarian": "^0.4.0"
    },
    "require-dev": {
        "laravel/sanctum": "^2.1",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Davidhsianturi\\Compass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Davidhsianturi\\Compass\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Davidhsianturi\\Compass\\CompassServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
